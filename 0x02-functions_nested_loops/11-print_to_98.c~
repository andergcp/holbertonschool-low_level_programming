B#include <stdio.h>
#include "holberton.h"
/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */
void print_to_98(int n)

{
	if (n <= 98)
	{
		for (n = n; n <= 98; n++)
		{
			printf("%i", n);
			if (n != 98)
				printf(", ");
		}
	}
	else if (n >= 98)
	{
		for (n = n; n >= 98; n--)
		{
			printf("%i", n);
			if (n != 98)
				printf(", ");
		}
	}
	printf("\n");
}
